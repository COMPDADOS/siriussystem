    function preencherCondominio( nId )
    {
        url = "{{ route('condominio.carga')}}/"+$("#I-IMB_IMB_IDMASTER").val();
        console.log( 'condominios: '+url );
        $.getJSON( url, function( data )

        {
            $("#IMB_CND_ID").empty();
            $("#IMB_CND_ID").append( '<option value=""></option>' );
            for( nI=0;nI < data.length;nI++)
