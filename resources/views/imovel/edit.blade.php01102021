                upLoadImagem();
            });

        $("#galeria-imagem-btn").click(function()
        {
            $("#galeria-imagem-upload").click();
        });

        if( $("#i-somenteleitura").val() == 'readonly')
        {
                        
            $("#i-btn-cancelar").html('OK');
